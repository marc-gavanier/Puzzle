{
  "manifest_version": 3,
  "name": "Solver",
  "version": "2.0",
  "description": "plugin to solve puzzles",
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://127.0.0.1/"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}